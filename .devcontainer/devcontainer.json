{
	"name": "CalculoFrete",
	"build": {
		"dockerfile": "Dockerfile",
		"args": {
			"VARIANT": "17",
			"INSTALL_NODE": "false",
			"NODE_VERSION": "lts/*",
			"INSTALL_MAVEN": "true",
		}
	},
	"settings": {
		"terminal.integrated.shell.linux": "/bin/bash",
		"java.home": "/docker-java-home"
	},
	"extensions": [
		"vscjava.vscode-java-pack"
	],
	"postStartCommand": "bash -i /check.sh > .availability.log"
}